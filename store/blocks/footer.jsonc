    ],
    "props":{
      "horizontalAlign": "center",
      "width":"33%"
    }
  },

  "link#foo2": {
    "props": {
      "href": "/login?returnUrl={queryString.returnUrl}",
      "label": "Home"
    }
  },
  "image#2":{
    "props": {
      "width": 50,
      "src": "assets/homeg.png",
      "link": "https://api.whatsapp.com/send?l=pt-BR&phone=5511964335064&text=Para%20falar%20com%20a%20Gabi%F0%9F%91%B1%F0%9F%8F%BB%E2%80%8D%E2%99%80%EF%B8%8F%0A%0AClique%20enviar%F0%9F%91%89%0A%0A------------%0A*D%C3%BAvida%20sobre%3A*%20Trof%C3%A9u%20Bras%C3%A3o%20Grandes%20Guerreiros%0A*ID%3A*%2052292"
  }
},
"rich-text#2":{
  "props":{
    "text":"Home"
  }
},

  "flex-layout.col#wats":{
    "children":[
      "rich-text#3",
      "image#3"
