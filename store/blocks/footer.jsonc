{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "footer-layout.mobile": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "flex-layout.row#footer": { 
    "children": [
      "flex-layout.col#entrar",
      "flex-layout.col#api",
      "flex-layout.col#wats"
    ],
    "props":{
      "horizontalAlign":"center",
      "width":"100%"
    }
  },

  "flex-layout.col#entrar":{
    "children":[
      "rich-text#1",
      "image#1"
    ],
    "props":{
      "horizontalAlign": "center",
      "width":"34%"
